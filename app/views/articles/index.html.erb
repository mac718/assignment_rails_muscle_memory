<h1>Article Index</h1>
<table>
  <thead>
    <th>Article ID</th>
    <th>Article Title</th>
  </thead>
  <% @articles.each do |article| %>
    <tr>
      <td><%= link_to "#{article.id}", article_path(article.id) %></td>
      <td><%= link_to "#{article.title}", article_path(article.id) %></td>
      <td><%= link_to "Edit this article", edit_article_path(article.id) %></td>
    </tr>
  <% end %>
</table>
